  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "manpacker:test-build",
      "program": "${workspaceRoot}/bin/manpacker-vue",
      "restart": true,
      "args": [
        "build",
        "--ic", "./test/config/manpacker.ic.js",
        "--config", "./test/manpacker.config.js"
      ],
      "cwd": "${workspaceRoot}"
    }, {
      "type": "node",
      "request": "launch",
      "name": "manpacker:test-server",
      "program": "${workspaceRoot}/bin/manpacker-vue",
      "restart": true,
      "args": [
        "server",
        "--ic", "./test/config/manpacker.ic.js",
        "--config", "./test/manpacker.config.js"
      ],
      "cwd": "${workspaceRoot}"
    }
  ]
}
